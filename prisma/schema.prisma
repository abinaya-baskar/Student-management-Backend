// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Define your model here
model Student {
  roll_no String @id @unique
  name    String
  class   String
  dob     String
  age     String
  password String
}
